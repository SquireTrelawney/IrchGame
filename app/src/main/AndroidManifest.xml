<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.IrchGame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lotos"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ind"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:screenOrientation="landscape"
        >
        <activity
            android:name=".shadows_puzzle"
            android:screenOrientation="landscape"
            ></activity>
        <activity
            android:name=".Separation_Puzzle"
            android:screenOrientation="landscape"
            />
        <activity android:name=".Puzzles" />
        <activity
            android:name=".Store_Puzzle"
            android:screenOrientation="landscape"
            ></activity>
        <activity
            android:name=".StoryMap"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>