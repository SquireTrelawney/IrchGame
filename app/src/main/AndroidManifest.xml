<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.IrchGame">

    <application
        android:allowBackup="true"
        android:icon="@drawable/lotos"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ind"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Separation_Puzzle"></activity>
        <activity android:name=".Puzzles" />
        <activity android:name=".Store_Puzzle"></activity>
        <activity android:name=".StoryMap" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>